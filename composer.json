{
    "name": "michelmelo/laravel-passes",
    "description": "Laravel library for creating iOS and Android Wallet Passes",
    "keywords": [
        "michelmelo",
        "laravel",
        "passes",
        "wallet",
        "apple",
        "google",
        "android",
        "ios"
    ],
    "homepage": "https://github.com/michelmelo/laravel-passes",
    "license": "MIT",
    "authors": [
        {
            "name": "Michel michel",
            "email": "eu@michelmelo.pt",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "chiiya/passes": "^0.5",
        "illuminate/contracts": "^7.0|^8.0|^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.11"
    },
    "require-dev": {
        "chiiya/laravel-code-style": "^1.6|^2.0",
        "nunomaduro/collision": "^6.1|^7.0",
        "orchestra/testbench": "^7.1|^8.0",
        "phpunit/phpunit": "^9.5|^10.0"
    },
    "autoload": {
        "psr-4": {
            "MichelMelo\\LaravelPasses\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichelMelo\\LaravelPasses\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichelMelo\\LaravelPasses\\LaravelPassesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
